    <extensions defaultExtensionNs="com.intellij">
        <completion.confidence language="JAVA"
                               implementationClass="com.launchdarkly.intellij.codeInsight.JavaCompletionConfidence"
                               order="before javaSkipAutopopupInStrings"/>
